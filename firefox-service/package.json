{
  "name": "firefox-cookie-service",
  "version": "1.0.0",
  "description": "Firefox-based cookie management service for video downloaders",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
